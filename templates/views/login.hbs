<form action="/login" method="post"
    class="container position-absolute top-50 start-50 translate-middle form-floating mb-3">
    <div class="mb-3 container w-75">
        <label for="formGroupExampleInput" class="form-label fs-4 tokenLable">Username: </label>
        <input type="text" name="username" class="form-control p-2 input-btn fs-4 tokenInput" autocomplete="off"
            id="formGroupExampleInput" placeholder="Username">
        <label for="formGroupExampleInput" class="form-label  mt-3 fs-4 tokenLable">Password: </label>
        <input type="password" name="password" class="form-control input-btn p-2 fs-4 tokenInput mb-2"
            id="formGroupExampleInput" placeholder="Password">
        <pre class="text-danger fs-5 py-1">{{msg}}</pre>
        <pre class="text-danger fs-5 py-1">{{msgs}}</pre>
    </div>
    <button type="submit"
        class="btn btn-primary mt-5 py-3 fs-4 position-relative w-50 top-50 start-50 translate-middle text-center start-btn">Log
        In</button>
</form>

